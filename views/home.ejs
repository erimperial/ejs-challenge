<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= startingContent %></p>

<% if (postContent) { %>
<% postContent.forEach(post => { %>
<h1><%= post.title %></h1>
<p><%= post.postBody.substring(0, 101) %>... <a href=<%= post.titleUrl %>>Read More</a></p>
<% }) %>
<% } %>

<%- include("partials/footer") -%>
